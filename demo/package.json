{
  "private": true,
  "name": "demo",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "deploy": "gh-pages -t -d dist",
    "eslint": "eslint pages/** components/**",
    "preexport": "rimraf dist && NEXT_PUBLIC_BASE_PATH=/ace-tracer yarn build",
    "export": "NEXT_PUBLIC_BASE_PATH=/ace-tracer next export -o dist && touch dist/.nojekyll",
    "lint": "next lint",
    "serve": "next start",
    "start": "next dev",
    "prettier": "prettier -w --cache --plugin=prettier-plugin-organize-imports . && prettier-package-json --write"
  },
  "dependencies": {
    "@cs124/ace-recorder": "2023.9.0",
    "glob-promise": "6.0.5",
    "lodash": "4.17.21",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-ace": "10.1.0",
    "react-compound-timer": "2.0.5",
    "react-dom": "18.2.0",
    "runtypes": "6.7.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.198",
    "@types/react": "18.2.21",
    "@types/styled-components": "5.1.27",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-plugin-react-hooks": "4.6.0",
    "gh-pages": "6.0.0",
    "prettier": "3.0.3",
    "prettier-package-json": "gchallen/prettier-package-json#2023.8.0",
    "prettier-plugin-organize-imports": "3.2.3",
    "typescript": "5.2.2"
  }
}
