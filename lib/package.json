{
  "name": "@cs124/ace-recorder",
  "version": "2023.9.0",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "checker": "npm run prettier && npm run eslint && npm run tsc && npm run depcheck && npm run build",
    "clean": "rimraf dist",
    "depcheck": "depcheck --ignores=prettier-plugin-*,rimraf --ignore-dirs=dist",
    "eslint": "eslint src/**",
    "ncu": "ncu",
    "prettier": "prettier -w --cache --plugin=prettier-plugin-organize-imports . && prettier-package-json --write",
    "prepublish": "rimraf dist && tsc",
    "start": "yarn watch",
    "tsc": "tsc --noEmit",
    "watch": "tsc -w"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@cs124/ace-recorder-types": "2023.9.0",
    "fix-webm-duration": "1.0.5",
    "throttle-debounce": "5.0.0",
    "whatwg-mimetype": "3.0.0"
  },
  "peerDependencies": {
    "ace-builds": "1.24.2",
    "runtypes": ">= 6"
  },
  "devDependencies": {
    "@types/throttle-debounce": "5.0.0",
    "@types/whatwg-mimetype": "3.0.0",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "ace-builds": "1.24.2",
    "depcheck": "1.4.6",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "npm-check-updates": "16.14.2",
    "prettier": "3.0.3",
    "prettier-package-json": "gchallen/prettier-package-json#2023.8.0",
    "prettier-plugin-organize-imports": "3.2.3",
    "rimraf": "5.0.1",
    "runtypes": "6.7.0",
    "typed-emitter": "1.4.0",
    "typescript": "5.2.2"
  }
}
